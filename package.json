{
    "name": "@froxz/vite-plugin-s3",
    "version": "1.1.2",
    "description": "Allows you to upload files to S3",
    "type": "module",
    "keywords": [
        "vite",
        "s3",
        "vite-plugin"
    ],
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./*"
            ]
        }
    },
    "files": [
        "dist",
        "*.d.ts"
    ],
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./*": "./*"
    },
    "scripts": {
        "build": "npx tsup src/index.ts --dts --format cjs,esm"
    },
    "repository": "https://github.com/Froxz/vite-plugin-s3",
    "author": "Froxz(sergio11of@gmail.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Froxz/vite-plugin-s3/issues"
    },
    "homepage": "https://github.com/Froxz/vite-plugin-s3",
    "peerDependencies": {
        "vite": "^3.1.0 || ^4.0.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.0.0",
        "mime-types": "^2.1.35",
        "recursive-readdir": "^2.2.3"
    },
    "devDependencies": {
        "kolorist": "^1.7.0",
        "lodash": "^4.17.21",
        "tsup": "^6.6.3",
        "typescript": "^4.9.5"
    }
}
